	<select id="getHistoryRefNo" resultType="java.lang.String" parameterType="java.lang.String">
	    select "RefNo" from ${userSchema}."REFERENCE_INFO" 
			WHERE 1=1
		 <if test="searchType=='personal' ">
						and "FirstName"=#{firstName,jdbcType=VARCHAR}
						and "MidName" =#{midName,jdbcType=VARCHAR}
						and "LastName" =#{lastName,jdbcType=VARCHAR}
					
			</if>

			<if test="searchType=='corporation'">
			    
					and "CorporateName"=#{corporateName,jdbcType=VARCHAR}
			</if>
		 
			
	</select>
	
	<select id="getMoneyInHistory" resultType="com.liri.reference.model.BankInInfoDto"
	parameterType="java.lang.String">
			select "OriginalCurrency" ,"OriginalAmount" from ${walletTypeSchema}."IMP_BANK_IN_INFO" 
			    where "BankIn_ReceiveRefNo"=#{refNo,jdbcType=VARCHAR} and "BankIn_RemittanceBank"=#{fiSimpleName,jdbcType=VARCHAR}
	</select>
	
	<select id="getWalletSchemaByCompanyId" resultType="java.lang.String" parameterType="java.lang.String">
	        SELECT "WalletTypeSchema" from "MST_WALLETTYPE" 
			WHERE string_to_array("CompanyID",',') <![CDATA[ @> ]]> string_to_array(#{companyId,jdbcType=VARCHAR},',')
			and "IsDelete"=0
	</select>
